{
    "name": "IPWhois.io",
    "product_identifier": "9535a2b6-531e-4b7f-b57d-3a04368c9056",
    "brief_description": "IP Geolocation API",
    "description": "IP Geolocation API",
    "author": "Brian Carroll <brian@hasecuritysolutions.com>",
    "tags": ["observable", "event"],
    "category": ["Enrichment"],
    "enabled": false,
    "contributor": [
        "Justin Henderson <justin@hasecuritysolutions.com>"
    ],
    "license": "H&A Security Solutions - Premium Support",
    "version": "1.0.0",
    "manifest": {
        "actions": [
            {
                "friendly_name": "IP Lookup",
                "name": "ip_whois_lookup",
                "type": "action",
                "description": "Looks up the WHOIS information on an IP address",
                "tags": ["event", "observable"],
                "target_object": "event",
                "run_from": "console",
                "parameters": {
                    "ips": {
                        "label": "IP Addresses",
                        "type": "str-multiple",
                        "description": "List of IP addresses to look up",
                        "default_options_from": "observables",
                        "observable_data_type": "ip"
                    }
                },
                "trigger": ["event_rule", "manual"],
                "source_object_type": ["event","observable"],
                "configuration": {
                    "cache_results": {
                        "label": "Cache Results",
                        "type": "bool",
                        "required": false,
                        "default": true,
                        "description": "Cache the results of this action"
                    },
                    "cache_expiry": {
                        "label": "Cache Expiry (seconds)",
                        "type": "int",
                        "required": false,
                        "default": 3600,
                        "description": "The amount of time to cache the results of this action for"
                    }
                }
            }
        ],
        "configuration": {
            "api_key": {
                "label": "API Key",
                "type": "str",
                "required": false,
                "secret": true,
                "description": "The API key for IPWhois.io"
            }
        }
    },
    "logo": "data:image/png;base64,"
}