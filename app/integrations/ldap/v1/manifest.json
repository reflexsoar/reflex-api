{
    "name": "LDAP",
    "product_identifier": "9575e42d-01d6-451e-a8ef-5750d2bf3738",
    "brief_description": "A ReflexSOAR plugin for interacting with LDAP servers",
    "description": "A ReflexSOAR plugin for interacting with LDAP servers",
    "author": "Brian Carroll <brian@hasecuritysolutions.com>",
    "tags": [
        "observable",
        "event"
    ],
    "category": [
        "Enrichment"
    ],
    "enabled": false,
    "contributor": [
        "Justin Henderson <justin@hasecuritysolutions.com>"
    ],
    "license": "H&A Security Solutions - Premium Support",
    "version": "1.0.2",
    "manifest": {
        "actions": [
            {
                "friendly_name": "Synchronize LDAP",
                "name": "sync_ldap_users",
                "description": "Polls an LDAP server for a list of objects.  This action will run from an `agent`, so at least one agent must exist with the `runner` role.",
                "configuration": {
                    "search_base": {
                        "label": "Search Base",
                        "type": "str",
                        "required": true,
                        "secret": false,
                        "default": "",
                        "description": "The base DN to search from"
                    },
                    "search_filter": {
                        "label": "Search Filter",
                        "type": "str",
                        "required": true,
                        "secret": false,
                        "default": "",
                        "description": "The LDAP search filter to use"
                    },
                    "attributes": {
                        "label": "Object Attributes",
                        "type": "str-multiple",
                        "required": true,
                        "secret": false,
                        "default": ["sAMAccountName", "mail", "displayName", "memberOf"],
                        "description": "The attributes to return"
                    },
                    "target": {
                        "label": "Synchronization Target",
                        "type": "str-select",
                        "required": true,
                        "secret": false,
                        "default": "inventory-user",
                        "description": "The target to synchronize values to",
                        "options": [
                            {
                                "label": "Intel List",
                                "value": "intel-list"
                            },
                            {
                                "label": "Computer Inventory",
                                "value": "inventory-computer"
                            },
                            {
                                "label": "User Inventory",
                                "value": "inventory-user"
                            },
                            {
                                "label": "Group Inventory",
                                "value": "inventory-group"
                            }
                        ]
                    },
                    "intel_list": {
                        "label": "Intel List",
                        "type": "str-select",
                        "required": false,
                        "secret": false,
                        "default": "",
                        "description": "The Intel List to synchronize to",
                        "conditions": [
                            {
                                "field": "target",
                                "operator": "eq",
                                "value": "intel-list"
                            }
                        ]
                    },
                    "generate_intel_list": {
                        "label": "Generate Intel List",
                        "type": "bool",
                        "required": false,
                        "secret": false,
                        "default": false,
                        "description": "Whether to generate the Intel List if it does not exist",
                        "conditions": [
                            {
                                "field": "target",
                                "operator": "eq",
                                "value": "intel-list"
                            }
                        ]
                    }
                },
                "run_from": "agent",
                "type": "inventory",
                "trigger": [
                    "schedule"
                ]
            }
        ],
        "configuration": {
            "server": {
                "label": "LDAP Server",
                "type": "str",
                "required": true,
                "secret": false,
                "default": "127.0.0.1",
                "description": "The LDAP server to connect to"
            },
            "port": {
                "label": "LDAP Port",
                "type": "int",
                "required": true,
                "secret": false,
                "default": 636,
                "description": "The port to connect to"
            },
            "ca_cert": {
                "label": "CA Certificate",
                "type": "text",
                "required": false,
                "secret": false,
                "default": "",
                "description": "The CA certificate to use"
            },
            "username": {
                "label": "Username",
                "type": "str",
                "required": true,
                "secret": false,
                "default": "Administrator",
                "description": "The username to use.  Enter the CER file contents here"
            },
            "password": {
                "label": "Password",
                "type": "str",
                "required": true,
                "secret": true,
                "default": "",
                "description": "The password to use"
            }
        }
    },
    "logo": "data:image/png;base64,"
}