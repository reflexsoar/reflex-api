{
    "name": "OpenSearch",
    "product_identifier": "2b9a18b0-a2ce-49b5-a6f2-0456efe97414",
    "brief_description": "A ReflexSOAR plugin for interacting with OpenSearch",
    "description": "A ReflexSOAR plugin for interacting with OpenSearch",
    "author": "Brian Carroll <brian@hasecuritysolutions.com>",
    "tags": [
        "observable",
        "event"
    ],
    "category": [
        "Enrichment"
    ],
    "enabled": false,
    "contributor": [
        "Justin Henderson <justin@hasecuritysolutions.com>"
    ],
    "license": "H&A Security Solutions - Premium Support",
    "version": "1.0.0",
    "manifest": {
        "actions": [
            {
                "friendly_name": "Poll Index",
                "name": "poll_index",
                "description": "Polls an index for new data",
                "configuration": {
                    "index": {
                        "label": "Index Pattern",
                        "type": "str",
                        "required": true,
                        "secret": false,
                        "default": "",
                        "description": "The index pattern to poll"
                    },
                    "poll_interval": {
                        "label": "Poll Interval (seconds)",
                        "type": "int",
                        "required": true,
                        "secret": false,
                        "default": 60,
                        "description": "The interval in minutes to poll for new data"
                    },
                    "search_filter": {
                        "label": "Search Filter (lucene)",
                        "type": "str",
                        "required": true,
                        "secret": false,
                        "default": "",
                        "description": "The search filter to use"
                    }
                },
                "run_from": "agent",
                "type": "input",
                "trigger": [
                    "schedule",
                    "manual"
                ]
            }
        ],
        "configuration": {
            "authentication_method": {
                "label": "Authentication Method",
                "type": "str-select",
                "required": true,
                "secret": false,
                "default": "basic",
                "description": "The authentication method to use",
                "options": [
                    {
                        "label": "Basic Authentication",
                        "value": "basic"
                    },
                    {
                        "label": "API Key",
                        "value": "api_key"
                    }
                ]
            },
            "username": {
                "label": "Username",
                "type": "str",
                "required": true,
                "secret": true,
                "default": "",
                "description": "The username to use for authentication.  If using API Auth, this is the API Key"
            },
            "password": {
                "label": "Password",
                "type": "str",
                "required": true,
                "secret": true,
                "default": "",
                "description": "The password to use for authentication. If using API Auth, this is the API Secret"
            }
        }
    },
    "logo": "data:image/png;base64,"
}