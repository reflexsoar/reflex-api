{
    "name": "Microsoft Sysmon",
    "product_identifier": "dd3f1684-15f8-4410-9e87-821e4f82e4f5",
    "brief_description": "Install, update, disable Microsoft Sysmon agents.  Create and manage Sysmon configurations.",
    "description": "Install, update, disable Microsoft Sysmon agents.  Create and manage Sysmon configurations.",
    "author": "Brian Carroll <brian@hasecuritysolutions.com>",
    "tags": ["event"],
    "category": ["Operations","Logging"],
    "enabled": false,
    "contributor": [
        "Justin Henderson <justin@hasecuritysolutions.com>"
    ],
    "license": "H&A Security Solutions - Premium Support",
    "version": "1.0.0",
    "manifest": {
        "actions": [
            {
                "friendly_name": "Manage Configuration",
                "name": "manage_configuration",
                "type": "action",
                "description": "Installs or updates an existing Sysmon configuration.",
                "tags": ["agent", "sysmon"],
                "run_from": "agent",
                "agent_role": "sysmon_manager",
                "parameters": {
                    
                },
                "trigger": ["system"],
                "configuration": {
                    "name": {
                        "label": "Name",
                        "type": "str",
                        "required": true,
                        "description": "The friendly name of this configuration"
                    },
                    "config_block": {
                        "label": "Configuration",
                        "type": "text",
                        "rows": 10,
                        "required": true,
                        "description": "The Sysmon XML Configuration",
                        "is_code": true
                    },
                    "target_version": {
                        "label": "Target Versions",
                        "type": "str",
                        "required": true,
                        "description": "This configuration will only be applied to Sysmon agents with this version or higher"
                    },
                    "target_architecture": {
                        "label": "Target Architecture",
                        "type": "str-select",
                        "required": true,
                        "options": [
                            {
                                "label": "x86",
                                "value": "x86"
                            },
                            {
                                "label": "x64",
                                "value": "x64"
                            }
                        ],
                        "description": "This configuration will only be applied to Sysmon agents with this architecture"
                    }
                }
            },
            {
                "friendly_name": "Install or Update Sysmon",
                "name": "install_or_update",
                "type": "action",
                "description": "Installs or updates an existing Sysmon installation.",
                "tags": ["agent", "sysmon"],
                "run_from": "agent",
                "agent_role": "sysmon_manager",
                "parameters": {
                    
                },
                "trigger": ["system"],
                "configuration": {
                    "installer_path": {
                        "label": "Installer Path",
                        "type": "str",
                        "required": true,
                        "description": "A URL or UNC path to the Sysmon installation files."
                    },
                    "target_version": {
                        "label": "Target Versions",
                        "type": "str",
                        "secret": false,
                        "required": true,
                        "description": "This configuration will only be applied to Sysmon agents with this version or higher"
                    },
                    "install_checksum": {
                        "label": "Installer Checksum",
                        "type": "str",
                        "secret": false,
                        "required": true,
                        "description": "The SHA1 checksum of the installer file to ensure the download was not corrupted or tampared with."
                    },
                    "target_architecture": {
                        "label": "Target Architecture",
                        "type": "str-select",
                        "required": true,
                        "options": [
                            {
                                "label": "x86",
                                "value": "x86"
                            },
                            {
                                "label": "x64",
                                "value": "x64"
                            }
                        ],
                        "description": "This configuration will only be applied to Sysmon agents with this architecture"
                    }
                }
            },
            {
                "friendly_name": "Disable Sysmon",
                "name": "disable",
                "type": "action",
                "description": "Disables Sysmon on the target system by stopping the service.  It will not be re-enabled until the service is started again.  If this policy is still active when Sysmon is started again, it will be disabled again.",
                "tags": ["agent", "sysmon"],
                "run_from": "agent",
                "agent_role": "sysmon_manager",
                "parameters": {
                    
                },
                "trigger": ["system"],
                "configuration": {
                    "target_version": {
                        "label": "Target Versions",
                        "type": "str",
                        "secret": false,
                        "required": true,
                        "description": "This configuration will only be applied to Sysmon agents with this version or higher"
                    },
                    "target_architecture": {
                        "label": "Target Architecture",
                        "type": "str-select",
                        "required": true,
                        "options": [
                            {
                                "label": "x86",
                                "value": "x86"
                            },
                            {
                                "label": "x64",
                                "value": "x64"
                            }
                        ],
                        "description": "This configuration will only be applied to Sysmon agents with this architecture"
                    }
                }
            }
        ],
        "configuration": {
            "evaluation_interval": {
                "label": "Evaluation Interval",
                "type": "int",
                "required": true,
                "description": "The number of minutes between each evaluation of the configuration",
                "default": 30,
                "min": 5,
                "max": 1440
            }
        }
    },
    "logo": "data:image/png;base64,"
}