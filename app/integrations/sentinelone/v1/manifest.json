{
    "name": "SentinelOne",
    "product_identifier": "3706b78e-d55c-4b93-87f6-4c21a19c9bdb",
    "brief_description": "A SentinelOne integration for ReflexSOAR",
    "description": "## Overview\nThe SentinelOne integration for ReflexSOAR allows for bi-directional communication between SOC teams and the SentinelOne management platform.  This integration allows for multiple actions ranging from collecting asset information, resolving threats, or active mitigation.\n## Use Cases\n- Collect asset inventory to enrich Detections with host information\n- Collect threats from the SentinelOne console to centralize alert triage",
    "author": "Brian Carroll <brian@hasecuritysolutions.com>",
    "tags": ["response","inventory","threat"],
    "category": ["Endpoint Detection"],
    "enabled": false,
    "contributor": [
        "Justin Henderson <justin@hasecuritysolutions>"
    ],
    "license": "H&A Security Solutions - Premium Support",
    "version": "1.0.1",
    "manifest": {
        "actions": [
            {
                "friendly_name": "Synchronize Inventory",
                "description": "Synchronize the SentinelOne inventory with ReflexSOAR's Asset Inventory",
                "name": "synchronize_inventory",
                "type": "inventory",
                "run_from": "agent",
                "configuration": {
                    "sync_interval": {
                        "label": "Synchronization Interval (minutes)",
                        "type": "int",
                        "required": true,
                        "secret": false,
                        "default": 60,
                        "description": "The interval in minutes to synchronize the inventory"
                    },
                    "sync_mode": {
                        "label": "Synchronization Mode",
                        "type": "str-select",
                        "required": true,
                        "secret": false,
                        "description": "The mode to synchronize the inventory in.  A full sync will synchronize all assets, while an incremental sync will only synchronize assets that have changed since the last sync.",
                        "default": "full",
                        "options": [
                            {
                                "label": "Full",
                                "value": "full"
                            },
                            {
                                "label": "Incremental",
                                "value": "incremental"
                            }
                        ]
                    },
                    "conflict_resolution": {
                        "label": "Conflict Resolution",
                        "type": "str-select",
                        "required": true,
                        "secret": false,
                        "description": "The method to use to resolve conflicts. A conflict occurs when an asset is found in SentinelOne and ReflexSOAR, but the asset has different information in each system.  The default is to use the information from SentinelOne.",
                        "default": "sentinelone",
                        "options": [
                            {
                                "label": "SentinelOne",
                                "value": "sentinelone"
                            },
                            {
                                "label": "ReflexSOAR",
                                "value": "reflexsoar"
                            }
                        ]
                    },
                    "host_identifier": {
                        "label": "Host Identifier",
                        "type": "str-select",
                        "required": true,
                        "secret": false,
                        "description": "The method to use to identify hosts. The default is to use the hostname.",
                        "default": "hostname",
                        "options": [
                            {
                                "label": "Hostname",
                                "value": "hostname"
                            },
                            {
                                "label": "MAC Address",
                                "value": "mac"
                            },
                            {
                                "label": "IP Address",
                                "value": "ip"
                            }
                        ]
                    }
                }
            },
            {
                "friendly_name": "Isolate Host",
                "description": "Isolate a host from the network",
                "name": "isolate_host",
                "parameters": {
                    "hostname": {
                        "label": "Hostname",
                        "type": "str-select",
                        "required": false,
                        "default_options_from": "observables",
                        "observable_data_type": "host",
                        "description": "The Hostname of the agent to isolate"
                    },
                    "mac": {
                        "label": "MAC Address",
                        "type": "str-select",
                        "required": false,
                        "default_options_from": "observables",
                        "observable_data_type": "mac",
                        "description": "The MAC Address of the agent to isolate"
                    },
                    "ip": {
                        "label": "IP Address",
                        "type": "str-select",
                        "required": false,
                        "default_options_from": "observables",
                        "observable_data_type": "ip",
                        "description": "The Hostname of the agent to isolate"
                    },
                    "tag_on_success": {
                        "label": "Tag On Success",
                        "type": "bool",
                        "required": true,
                        "default": true,
                        "description": "Tag the host with the \"SentinelOne Isolated\" tag on success"
                    }
                },
                "type": "action",
                "run_from": "console",
                "trigger": ["event_rule", "manual"],
                "source_object_type": ["observable", "event"],
                "observable_type": ["host"]
            },
            {
                "friendly_name": "Reconnect Host",
                "description": "Reconnect a host to the network",
                "name": "reconnect_host",
                "parameters": {
                    "hostname": {
                        "label": "Hostname",
                        "type": "str",
                        "required": true,
                        "description": "The Hostname of the agent to reconnect"
                    }
                },
                "type": "action",
                "run_from": "console",
                "trigger": ["event_rule", "manual"],
                "source_object_type": ["observable", "event"],
                "observable_type": ["host"]
            },
            {
                "friendly_name": "Get Threat Details",
                "description": "Get details about a SentinelOne threat",
                "name": "get_threat_details",
                "parameters": {
                    "ids": {
                        "label": "Threat ID",
                        "type": "str",
                        "required": true,
                        "description": "The ID of the threat to get details for"
                    }
                },
                "type": "action",
                "run_from": "console",
                "trigger": ["event_rule", "manual"],
                "source_object_type": ["event"]
            },
            {
                "friendly_name": "Search Deep Visibility",
                "description": "Search the Deep Visibility logs for a specific query",
                "name": "search_deep_visibility",
                "parameters": {
                        "query": {
                        "label": "Query",
                        "type": "str",
                        "required": true,
                        "description": "The query to search for"
                    }
                },
                "type": "action",
                "run_from": "console",
                "trigger": ["event_rule", "manual"],
                "source_object_type": [
                    "observable",
                    "event"
                ],
                "observable_type": [
                    "ip",
                    "domain",
                    "url",
                    "hash",
                    "file",
                    "host"
                ]
            },
            {
                "friendly_name": "Get Agent Details",
                "description": "Get details about a SentinelOne agent",
                "name": "get_agent_details",
                "parameters": {
                    "hostname": {
                        "label": "Hostnames",
                        "type": "str-select",
                        "required": false,
                        "default_options_from": "observables",
                        "observable_data_type": "host",
                        "description": "The Hostname of the agent to isolate"
                    },
                    "mac": {
                        "label": "MAC Addresses",
                        "type": "str-select",
                        "required": false,
                        "default_options_from": "observables",
                        "observable_data_type": "mac",
                        "description": "The MAC Address of the agent to isolate"
                    },
                    "ip": {
                        "label": "IP Addresses",
                        "type": "str-select",
                        "required": false,
                        "default_options_from": "observables",
                        "observable_data_type": "ip",
                        "description": "The Hostname of the agent to isolate"
                    },
                    "hostname_source_fields": {
                        "label": "Hostname Source Fields",
                        "type": "str-multiple",
                        "required": false,
                        "default": [],
                        "description": "A source field to fetch the hostnames from"
                    },
                    "mac_source_fields": {
                        "label": "MAC Source Fields",
                        "type": "str-multiple",
                        "required": false,
                        "default": [],
                        "description": "A source field to fetch the MAC addresses from"
                    },
                    "ip_source_fields": {
                        "label": "IP Source Fields",
                        "type": "str-multiple",
                        "required": false,
                        "default": [],
                        "description": "A source field to fetch the IP addresses from"
                    }
                },
                "type": "action",
                "run_from": "console",
                "trigger": ["event_rule", "manual"],
                "source_object_type": ["observable", "event"],
                "observable_type": ["host", "mac", "ip"],
                "configuration": {
                    "cache_results": {
                        "label": "Cache Results",
                        "type": "bool",
                        "required": false,
                        "default": true,
                        "description": "Cache the results of this action"
                    },
                    "cache_expiry": {
                        "label": "Cache Expiry (seconds)",
                        "type": "int",
                        "required": false,
                        "default": 3600,
                        "description": "The amount of time to cache the results of this action for"
                    }
                }
            },
            {
                "friendly_name": "Add Threat Note",
                "description": "Add a note to a threat",
                "name": "add_threat_note",
                "parameters": {
                    "ids": {
                        "label": "Threat ID",
                        "type": "str",
                        "required": true,
                        "description": "The ID of the threat to add a note to"
                    },
                    "note": {
                        "label": "Note",
                        "type": "str",
                        "required": true,
                        "description": "The note to add to the threat"
                    }
                },
                "type": "action",
                "run_from": "console",
                "trigger": ["event_rule", "manual"],
                "source_object_type": ["event"]
            },
            {
                "friendly_name": "Poll Threats",
                "description": "Poll for new threats",
                "name": "poll_threats",
                "run_from": "agent",
                "type": "input",
                "configuration": {
                    "poll_interval": {
                        "label": "Poll Interval (minutes)",
                        "type": "int",
                        "required": true,
                        "secret": false,
                        "default": 60,
                        "description": "The interval in minutes to poll for new threats"
                    }
                },
                "trigger": ["schedule", "manual"]
            }
        ],
        "configuration": {
            "api_key": {
                "label": "API Key",
                "type": "str",
                "required": true,
                "secret": true,
                "description": "The API key for SentinelOne"
            },
            "api_url": {
                "label": "API URL",
                "type": "str",
                "required": true,
                "secret": false,
                "description": "The URL for the SentinelOne API"
            }
        }
    },
    "logo": "data:image/png;base64,"
}